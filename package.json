{
  "name": "zchat",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"worker,ui\" -c \"#f6821e,#7ce645\" \"bun run dev:worker\" \"bun run dev:ui\"",
    "dev:ui": "cd src/ui && vinxi dev",
    "build:ui": "cd src/ui && vinxi build",
    "start:ui": "cd src/ui && vinxi start",
    "dev:worker": "cd src/worker && bunx wrangler dev"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250506.0",
    "@types/bun": "latest",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.14.1"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.120.2",
    "@tanstack/react-start": "^1.120.2",
    "openai": "^4.97.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vinxi": "^0.5.6"
  }
}
